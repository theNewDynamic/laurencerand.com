{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
      "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@util/*": [
        "src/util/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@tw": [
        "src/styles/tailwind.css"
      ],
    },
  }
}
